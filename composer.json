    "type": "library",
    "version": "1.0.0",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "*",
        "illuminate/support": "*",
        "illuminate/http": "*",
        "illuminate/queue": "*",
        "illuminate/filesystem": "*"
    },
    "license": "MIT",
    "autoload": {
